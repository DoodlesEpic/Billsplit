{
	"name": "billsplit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@smui/button": "^8.0.0-beta.3",
		"@smui/card": "^8.0.0-beta.3",
		"@smui/drawer": "^8.0.0-beta.3",
		"@smui/fab": "^8.0.0-beta.3",
		"@smui/icon-button": "^8.0.0-beta.3",
		"@smui/list": "^8.0.0-beta.3",
		"@smui/textfield": "^8.0.0-beta.3",
		"@smui/top-app-bar": "^8.0.0-beta.3",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.5",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^8.36.0",
		"@typescript-eslint/parser": "^8.36.0",
		"@vite-pwa/assets-generator": "^1.0.0",
		"@vite-pwa/sveltekit": "^1.0.0",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"smui-theme": "^8.0.0-beta.3",
		"svelte": "^5.35.6",
		"svelte-check": "^4.2.2",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^7.0.4",
		"vite-plugin-pwa": "^1.0.1"
	},
	"type": "module",
	"packageManager": "yarn@4.9.2"
}
