{
	"name": "billsplit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.15",
		"@smui/card": "^7.0.0-beta.16",
		"@smui/fab": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/textfield": "^7.0.0-beta.15",
		"@smui/top-app-bar": "^7.0.0-beta.15",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.7",
		"@typescript-eslint/eslint-plugin": "^6.13.2",
		"@typescript-eslint/parser": "^6.13.2",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.15",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.1",
		"vite-plugin-pwa": "^0.16.7"
	},
	"type": "module",
	"packageManager": "yarn@4.0.0"
}
