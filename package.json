{
	"name": "billsplit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run smui-theme-light",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.16",
		"@smui/card": "^7.0.0-beta.16",
		"@smui/fab": "^7.0.0-beta.16",
		"@smui/icon-button": "^7.0.0-beta.16",
		"@smui/textfield": "^7.0.0-beta.16",
		"@smui/top-app-bar": "^7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.0.1",
		"@sveltejs/kit": "^2.0.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"@vite-pwa/sveltekit": "^0.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vite-plugin-pwa": "^0.17.4"
	},
	"type": "module",
	"packageManager": "yarn@4.0.2"
}
